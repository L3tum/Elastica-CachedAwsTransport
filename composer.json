  "description": "AWS Credential Cache for Elastica",
  "keywords": [
    "search",
    "client",
    "cache"
  ],
  "homepage": "http://elastica.io/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Ruflin",
      "homepage": "http://ruflin.com/"
    },
    {
      "name": "L3tum"
    }
  ],
  "require": {
